{
  "name": "atom-darklite",
  "main": "./lib/index",
  "version": "0.0.3",
  "description":
    "A single liteweight package for developing webpages with react and coffeescript",
  "keywords": ["react", "coffeescript"],
  "activationCommands": {
    "atom-workspace": ["atom-darklite:toggle"]
  },
  "repository": "https://github.com/jhessin/atom-darklite",
  "scripts": {
    "start": "coffee -o lib/ --no-header -cbw src/",
    "postinstall": "coffee -o lib/ --no-header -cb src/"
  },
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom": "^1.1.0",
    "atom-space-pen-views": "^2.2.0",
    "coffeescript": "^2.2.1",
    "figlet": "^1.2.0",
    "prettier": "^1.10.2"
  },
  "deserializers": {
    "atom-darklite/CoffeeTranspiler": "deserializeCoffeeTranspiler"
  }
}
